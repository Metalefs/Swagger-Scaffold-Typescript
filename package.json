{
  "name": "swagger-scaffold-typescript",
  "displayName": "SwaggerScaffold Typescript",
  "description": "Quickly create complex folder structures in your VS Code projects with ease! This extension allows you to generate folder and file hierarchies using simple text input, supporting indented tree format.",
  "version": "0.0.8",
  "publisher": "MetaBuild",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./main/extension.js",
  "contributes": {
    "commands": [      
      {
        "command": "extension.swaggerScaffold",
        "title": "SwaggerScaffold: Generate API Structure"
      },
      {
        "command": "extension.processFolderStructure",
        "title": "SwaggerScaffold: Process Folder Structure"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.x",
    "@types/vscode": "^1.75.0",
    "eslint": "^7.32.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "vscode": "^1.1.37",
    "vscode-test": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Metalefs/Folder-Structure-Generator-With-Template.git"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "node-html-parser": "^7.0.1"
  }
}
